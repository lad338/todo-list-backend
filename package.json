  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "concurrently": "^8.0.1",
    "eslint": "^8.37.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "tsc": "^2.0.4",
